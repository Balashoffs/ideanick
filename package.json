{
  "name": "ideanick",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Balashoffs/ideanick"
  },
  "scripts": {
    "types": "concurrently 'pnpm b types' 'pnpm w types'",
    "b": "pnpm --filter @ideanick/backend",
    "w": "pnpm --filter @ideanick/webapp",
    "prettify": "pnpm -r --parallel prettify",
    "prepare": "husky"
  },
  "private": true,
  "devDependencies": {
    "concurrently": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "types-node": "^1.6.0",
    "typescript": "^5.7.2"
  }
}
